                <td> <%= select_tag("OM_wood", options_for_select([], "1"), :class => "popup_cite_dropdown" ) %> </td>
                <td> <%= text_field :ecosystem, "OM_wood", :value => @ecosystem["OM_wood"].to_f, :class => "popup_value_field" %> </td>
              </tr>
              <tr id="OM_litter">
                <td> Litter (Mg/ha) </td>
                <td> <%= select_tag("OM_litter", options_for_select( [] , "1"), :class => "popup_cite_dropdown" ) %> </td>
                <td> <%= text_field :ecosystem, "OM_litter", :value => @ecosystem["OM_litter"].to_f, :class => "popup_value_field"  %> </td>
              </tr>
              <tr id="OM_peat">
                <td> Peat (Mg/ha) </td>
                <td> <%= select_tag("OM_peat", options_for_select( [] , "1"), :class => "popup_cite_dropdown" ) %> </td>
                <td> <%= text_field :ecosystem, "OM_peat", :value => @ecosystem["OM_peat"].to_f, :class => "popup_value_field"  %> </td>
              </tr>
