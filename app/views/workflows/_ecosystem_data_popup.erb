<div id="ecosystem_edit" class="well well-small">
  <h4 class="popup_heading"></h4>
  <hr />
  <!--  This will contain the hashed data for the current biome-->
  <div id="current_ecosystem_store"></div>
  <%= form_tag do %>
  <div class="container-fluid">
	  <div class="accordion" id="geo_refined_pft_accordion">
      <!-- ###### Initial Storage of Organic Matter ####### -->
      <div class="accordion-group">  
        <div class="accordion-heading">  
          <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseOne">  
            Initial Storage of Organic Matter
          </a>  
        </div>  
        <div id="collapseOne" class="accordion-body collapse" style="height: 0px;">  
          <div class="accordion-inner">
            <table class="table">
              <thead>
                <tr>
                  <th width="400px">Attribute</th>
                  <th width="300px">Source</th>
                  <th width="100px">Value</th>
                </tr>
              </thead>
              <!-- All of the below values are not used and are simply place holders for jquery to repopulate -->
              <tr id="OM_ag">
                <td> Aboveground biomass: live (Mg/ha) </td>
                <td> <%= select_tag("OM_ag", options_for_select( [] , "1"), :class => "popup_cite_dropdown" ) %> </td>
                <td> <%= text_field :ecosystem, "OM_ag", :value => @ecosystem["OM_ag"].to_f, :class => "popup_value_field" %> </td>
              </tr>
              <tr id="OM_root">
                <td> Root biomass (Mg/ha) </td>
                <td> <%= select_tag("OM_root", options_for_select( [] , "1"), :class => "popup_cite_dropdown" ) %> </td>
                <td> <%= text_field :ecosystem, "OM_root", :value => @ecosystem["OM_root"].to_f, :class => "popup_value_field" %> </td>
              </tr>
              <tr id="OM_wood">
